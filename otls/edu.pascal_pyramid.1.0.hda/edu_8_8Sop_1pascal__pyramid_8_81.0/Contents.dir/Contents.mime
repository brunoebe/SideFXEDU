--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/build_pyramid.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"detail"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"
int type = chi(\"type\");

int point=-1, value=1, value_prev=1;
int values[], lastRow[];
vector offset = {0, 0, 0};
int n, i, j;
vector startPos = {0, 0, 0};
float rowOffset = 1;
float layerOffsetY = -1;
float layerOffsetZ = -1/sqrt(3);

for(n=0; n<chi(\"nblayers\"); n++)
{
    offset.y = n*layerOffsetY;
  
    // compute the last row
    resize(lastRow, 0);
    lastRow[0]=1;
    for(int k=0; k<n+1; k++)
        lastRow[k+1] = lastRow[k] * (n-k)/(k+1);

    // loop over rows
    for(i=0; i<n+1; i++)
    {
        int maxcol = 1;
        if(type){
            maxcol = i+1;
            offset.z  = n*layerOffsetZ;
            offset.z += i*rowOffset; // for each row we add one row offset in z 
        }
        else{
            maxcol = 1;
            rowOffset = 2/sqrt(3); // length of equilateral triangle side when its height is 1: a = h*2/sqrt(3)
            offset.x  = n*layerOffsetZ;
            offset.x += i*rowOffset; // for each row we add one row offset in z 
        }
        
        // loop over columns
        values[0]=1;
        for(j=0; j<maxcol; j++)
        {
            values[j+1] = (values[j] * (i-j)/(j+1));
    
            if(type){
                offset.x = fit(j/float(i), 0.0, 1, -i/sqrt(3), i/sqrt(3));
            }

            point = addpoint(0, startPos + offset);
            setpointattrib(0, \"number\", point, values[j] * lastRow[i]);
            setpointattrib(0, \"nrow\", point, i);
            setpointattrib(0, \"ncol\", point, j);
        }
    }
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
type	[ 0	locks=0 ]	(	[ type	0 ] 	)
nblayers	[ 0	locks=0	autoscope=1 ]	(	[ nblayers	0 ] 	)
}

