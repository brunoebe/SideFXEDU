Content-Disposition: attachment; filename="hdaroot/verify_bsdf/sample_and_eval.parm"
Content-Type: text/plain

{
version 0.8
code	[ 0	locks=0	autoscope=0 ]	(	"vector  eval;
int     state;

// Store the albedo
$alb = luminance(albedo($f, $nvec));

// Weighted bsdf sampling (w_ prefix):
// w_v
// w_sample_eval
// w_sample_pdf
// w_eval_eval
// w_eval_pdf
sample_bsdf($f, $nvec, $w_v, eval, $w_sample_pdf, state, $P.x, $P.z);
$w_sample_eval = luminance(eval);
$w_eval_eval = luminance(eval_bsdf($f, $nvec, $w_v, $nvec, $w_eval_pdf, PBR_ALL_MASK));

// Isotropic bsdf sampling (u_ prefix)
// u_v
// u_sample_eval
// u_sample_pdf
// u_eval_eval
// u_eval_pdf
bsdf iso = isotropic();
